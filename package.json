{
  "name": "metavault-ai",
  "version": "1.0.0",
  "description": "MetaVault AI Monorepo (Agents + Contracts + Frontend)",
  "author": "Rohit_KK",
  "license": "MIT",
  "packageManager": "pnpm@10.18.3",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -C packages/frontend dev",
    "dev:agents": "pnpm -C packages/agents/defi-portfolio dev",
    "dev:contracts": "pnpm -C packages/contracts hardhat compile && pnpm -C packages/contracts hardhat node",
    "dev:all": "concurrently \"pnpm dev:contracts\" \"pnpm dev:agents\" \"pnpm dev\"",
    "build": "pnpm -C packages/frontend build",
    "build:frontend": "pnpm -C packages/frontend build",
    "build:agents": "pnpm -C packages/agents/defi-portfolio build",
    "build:contracts": "pnpm -C packages/contracts hardhat compile",
    "start": "pnpm start:agents",
    "start:frontend": "pnpm -C packages/frontend start",
    "start:agents": "pnpm -C packages/agents/defi-portfolio start",
    "contracts:compile": "pnpm -C packages/contracts hardhat compile",
    "contracts:test": "pnpm -C packages/contracts hardhat test",
    "contracts:deploy:local": "pnpm -C packages/contracts hardhat run scripts/deploy_mocks.ts --network localhost",
    "clean": "rimraf node_modules && rimraf packages/*/node_modules",
    "test": "echo \"No tests configured\" && exit 0"
  },
  "keywords": [
    "defi",
    "vault",
    "ai",
    "agent",
    "automation",
    "defi-portfolio",
    "agents",
    "contracts",
    "frontend"
  ],
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.3.0",
    "concurrently": "^9.1.0",
    "hardhat": "^3.0.11",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "autoprefixer": "^10.4.22",
    "ethers": "^6.15.0"
  }
}
